                        			</manifest>
                    			</archive>
                		</configuration-->
				<configuration>
					<compilerargs>
        					<arg>-g:source,lines,vars</arg>
      					</compilerargs>
					<source>11</source><target>11</target>
				</configuration>
			</plugin>
		</plugins>
	</build>

