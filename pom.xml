	<groupId>com.jdi.debugger</groupId>
	<artifactId>javadebugger</artifactId>
	<version>0.0.1-SNAPSHOT</version>
        <!--packaging>jar</packaging-->

	<dependencies>
		<dependency>
			<groupId>commons-io</groupId>
			<artifactId>commons-io</artifactId>
			<version>[2.5,)</version>
